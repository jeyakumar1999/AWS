Install-ADDSForest `
-CreateDnsDelegation:$false `
-DatabasePath "C:\Windows\NTDS" `
-DomainMode "Win2025" `
-DomainName "syd.ltd" `
-DomainNetbiosName "SYD" `
-ForestMode "Win2025" `
