Domain Service Installing st
Set-LocalUser -Name "Administrator" -Password (ConvertTo-SecureString -AsPlainText "9952017787@Aa" -Force)
Install-WindowsFeature -Name AD-Domain-Services, DNS -IncludeManagementTools            
$DSRMPasswordPlain = "9952017787@Aa"
$DSRMPasswordSecure = ConvertTo-SecureString -String $DSRMPasswordPlain -AsPlainText -Force
Import-Module ADDSDeployment
Install-ADDSForest `
-CreateDnsDelegation:$false `
-DatabasePath "C:\Windows\NTDS" `
-DomainMode "Win2025" `
-DomainName "syd.ltd" `
-DomainNetbiosName "SYD" `
-ForestMode "Win2025" `
-InstallDns:$true `
-LogPath "C:\Windows\NTDS" `
-NoRebootOnCompletion:$false `
-SafeModeAdministratorPassword $DSRMPasswordSecure `
-SysvolPath "C:\Windows\SYSVOL" `
-Force:$true

New-ADOrganizationalUnit -Name "syd" `
    -Path "DC=syd,DC=ltd" `
    -ProtectedFromAccidentalDeletion $true `
    -Description "Organizational Unit for New York Users and Resources"

$PlaintextPassword = "9952017787@Aa"
$InitialPassword = ConvertTo-SecureString -String $PlaintextPassword -AsPlainText -Force

# Sobhita Dhulipala

New-ADUser -Name "Sobhita" `
    -SamAccountName "Sobhita" `
    -UserPrincipalName "Sobhita@syd.ltd" `
    -Path "OU=syd,DC=syd,DC=ltd"  `
    -AccountPassword $InitialPassword `
    -Enabled $true `
    -ChangePasswordAtLogon $true `
    -GivenName "Sobhita" -Surname "Dhulipala"

# Riya Sen

    New-ADUser -Name "Riya" `
    -SamAccountName "Riya" `
    -UserPrincipalName "Riya@syd.ltd" `
