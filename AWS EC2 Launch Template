Enabling password for ssh via shell script 

#!/bin/bash
sudo su
sudo sed -i '/^#\?PasswordAuthentication/cPasswordAuthentication yes' /etc/ssh/sshd_config
sudo systemctl restart sshd
sudo useradd -m -s /bin/bash sobhita
echo "sobhita:9952017787@Aa" | sudo chpasswd#!/bin/bash

#!/bin/bash
sudo su
sudo sed -i '/^#\?PasswordAuthentication/cPasswordAuthentication yes' /etc/ssh/sshd_config
sudo systemctl restart sshd
sudo useradd -m -s /bin/bash tara
echo "tara:9952017787@Aa" | sudo chpasswd


