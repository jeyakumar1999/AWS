Mutiple script choice.

#!/bin/bash

echo "AWS ALPHA  ACCOUNT FOR OHIO REGION:"

PS3="Enter your Name (1-2): "  # Customize the prompt

select name in "LINUX" "WINDOWNS"; do
    case $name in
        "LINUX")
. Linux.sh
            break
            ;;
        "WINDOWNS")
. Windows.sh
    break
            ;;
        *)
            echo "Invalid choice. Please select a number from 1 to 2."
            ;;
    esac
done

